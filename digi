#include<stdio.h>
#include<string.h>

int has_common_digit(int x,int y){
    int digit_seen[10]={0};

    while(x>0){
        digit_seen[x%10]=1;
        x/=10;
    }
    if(y==0 && digit_seen[0]) return 1;

    while(y>0){
        if(digit_seen[y%10]) return 1;
        y/=10;
    }
    return 0;
}
int main()
{
    int t;
    scanf("%d",&t);

    while(t--){
        int x;
        scanf("%d",&x);

        for(int y=0;y<=1000;y++){
            if(has_common_digit(x,y)){
                printf("%d", y);
                break;
            }
        }
    }
    return 0;
    
}
